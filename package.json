{
  "name": "underscores",
  "version": "1.0.0",
  "description": "A WordPress starter theme.",
  "main": "index.js",
  "scripts": {
    "start": "webpack --watch",
    "build": "webpack --mode=production",
    "clean": "rm -r public/*",
    "eslint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "eslint:fix": "eslint \"src/**/*.{js,jsx}\" --quiet --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,scss}\"",
    "stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss}\" --fix",
    "wp-pot": "wp-pot --src '*.php' --dest-file 'languages/_s.pot' --domain '_s'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carrieforde/_s-with-webpack.git"
  },
  "keywords": [
    "wordpress",
    "webpack"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/carrieforde/_s-with-webpack/issues"
  },
  "homepage": "https://github.com/carrieforde/_s-with-webpack#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "autoprefixer": "^9.4.10",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.5",
    "browser-sync": "^2.26.3",
    "browser-sync-webpack-plugin": "^2.2.2",
    "css-loader": "^1.0.1",
    "css-mqpacker": "^7.0.0",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^3.6.0",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-prettier": "^2.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "img-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-wordpress": "^13.1.0",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.5.4",
    "stylelint-webpack-plugin": "^0.10.5",
    "svg-sprite-loader": "^4.1.3",
    "svgo": "^1.2.0",
    "svgo-loader": "^2.2.0",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "wp-pot-cli": "^1.2.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5"
  }
}
